TB_user 用户表
TB_role 角色表
TB_resources 资源表
TB_user_role 用户角色表
TB_role_resources 角色资源表
tb_loans   借款表  		          seq_tb_loans_id      序列             前期添加的有数据  序列可能不是从1开始的
tb_loansType  贷款类型表            seq_tb_loansType_id  序列
tb_investment 投资表    	          seq_tb_investment_id 序列
tb_resources   还款表	              seq_tb_resources_id   序列
tb_userInformation 用户信息表       seq_tb_userInformation_id  序列
tb_userAccount    账户信息表        seq_tb_userAccount_id  序列
tb_moneyRecord  资金记录表          seq_tb_moneyRecord_id   序列




    public static final int BID_APPLY = 0;// 审核失败
	public static final int BID_APPLY_FAIL = 1;// 未审核
	public static final int BID_PROGRESS = 2;// 招标中
	public static final int BID_FAIL = 3;// 流标
	public static final int BID_REPAY_PROGRESS = 4;// 还款中
	public static final int BID_FULL = 5;//满标审核通过
	public static final int BID_AUDIT_FIRST = 6;// 满标审核失败
	public static final int BID_AUDIT_SECOND = 7;//
	public static final int BID_AUDIT_FAIL = 8;//
	public static final int BID_AUDIT_SECOND_FAIL = 9;//
	public static final int BID_REPAY_EXCEED = 10;// 逾期
	public static final int BID_REPAY_FINASH = 11;// 还款结束
	public static final int BID_REPAY_HASREPAY = 12;// 本期已还款
